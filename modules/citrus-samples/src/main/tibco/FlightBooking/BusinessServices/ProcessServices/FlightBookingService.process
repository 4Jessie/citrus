<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:pfx="http://www.consol.com/schemas/FlightBooking/TravelAgency/TravelAgencySchema.xsd" xmlns:pfx2="http://www.consol.com/schemas/FlightBooking/AirlineSchema.xsd" xmlns:ns="http://www.tibco.com/pe/EngineTypes">
    <xsd:import namespace="http://www.consol.com/schemas/FlightBooking/AirlineSchema.xsd" schemaLocation="/SharedResources/SchemaDefinitions/XML/Airlines/AirlineSchema.xsd"/>
    <xsd:import namespace="http://www.consol.com/schemas/FlightBooking/TravelAgency/TravelAgencySchema.xsd" schemaLocation="/SharedResources/SchemaDefinitions/XML/TravelAgency/TravelAgencySchema.xsd"/>
    <pd:name>BusinessServices/ProcessServices/FlightBookingService.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType>
        <xsd:element name="Input">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element name="CorrelationId" type="xsd:string"/>
                    <xsd:element ref="pfx:Customer"/>
                    <xsd:element ref="pfx:BookingEntry" maxOccurs="unbounded"/>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:startType>
    <pd:startX>169</pd:startX>
    <pd:startY>175</pd:startY>
    <pd:returnBindings/>
    <pd:endName>End</pd:endName>
    <pd:endX>1224</pd:endX>
    <pd:endY>176</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1240485523165</pd:targetNamespace>
    <pd:group name="ForEachFlight">
        <pd:type>com.tibco.pe.core.LoopGroup</pd:type>
        <pd:resourceType>ae.process.group</pd:resourceType>
        <pd:x>283</pd:x>
        <pd:y>50</pd:y>
        <pd:width>544</pd:width>
        <pd:height>239</pd:height>
        <pd:collapsedWidth>57</pd:collapsedWidth>
        <pd:collapsedHeight>70</pd:collapsedHeight>
        <pd:groupVariables/>
        <pd:joinToggle>inherit</pd:joinToggle>
        <config>
            <pd:groupType>inputLoop</pd:groupType>
            <pd:serializable>false</pd:serializable>
            <pd:over>$Start/Input/pfx:BookingEntry</pd:over>
            <pd:indexSlot>bookingIndex</pd:indexSlot>
            <pd:outSlot>FlightBookingSuccess</pd:outSlot>
            <pd:activityOutputName>SendFlightBookingRequest</pd:activityOutputName>
            <pd:accumulateOutput>false</pd:accumulateOutput>
        </config>
        <pd:inputBindings/>
        <pd:expanded>true</pd:expanded>
        <pd:transition>
            <pd:from>start</pd:from>
            <pd:to>RenderFlightBookingRequest</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>SendFlightBookingRequest</pd:from>
            <pd:to>end</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>otherwise</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>RenderFlightBookingRequest</pd:from>
            <pd:to>SendFlightBookingRequest</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>SendFlightBookingRequest</pd:from>
            <pd:to>Generate Error</pd:to>
            <pd:xpathDescription>Failed</pd:xpathDescription>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>xpath</pd:conditionType>
            <pd:xpath>$SendFlightBookingRequest/Output/success = "false"</pd:xpath>
        </pd:transition>
        <pd:activity name="SendFlightBookingRequest">
            <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
            <pd:resourceType>ae.process.subprocess</pd:resourceType>
            <pd:x>571</pd:x>
            <pd:y>179</pd:y>
            <config>
                <processName>/IntegrationServices/Airlines/OutboundInterface.process</processName>
                <processNameXPath>concat("/IntegrationServices/Airlines/", $Start/Input/pfx:BookingEntry[$bookingIndex]/pfx:airline, "RequestSender.process")</processNameXPath>
            </config>
            <pd:inputBindings>
                <Input>
                    <CorrelationId>
                        <xsl:value-of select="$Start/Input/CorrelationId"/>
                    </CorrelationId>
                    <MessageBody>
                        <xsl:value-of select="$RenderFlightBookingRequest/xmlString"/>
                    </MessageBody>
                </Input>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="RenderFlightBookingRequest">
            <pd:type>com.tibco.plugin.xml.XMLRendererActivity</pd:type>
            <pd:resourceType>ae.activities.XMLRendererActivity</pd:resourceType>
            <pd:x>422</pd:x>
            <pd:y>177</pd:y>
            <config>
                <renderAsText>text</renderAsText>
                <validateInput>true</validateInput>
                <term ref="pfx2:FlightBookingRequestMessage"/>
            </config>
            <pd:inputBindings>
                <pfx2:FlightBookingRequestMessage>
                    <pfx2:correlationId>
                        <xsl:value-of select="$Start/Input/CorrelationId"/>
                    </pfx2:correlationId>
                    <pfx2:bookingId>
                        <xsl:value-of select="$bookingIndex"/>
                    </pfx2:bookingId>
                    <pfx2:customer>
                        <pfx2:id>
                            <xsl:value-of select="$Start/Input/pfx:Customer/pfx:id"/>
                        </pfx2:id>
                        <pfx2:firstname>
                            <xsl:value-of select="$Start/Input/pfx:Customer/pfx:firstname"/>
                        </pfx2:firstname>
                        <pfx2:lastname>
                            <xsl:value-of select="$Start/Input/pfx:Customer/pfx:lastname"/>
                        </pfx2:lastname>
                    </pfx2:customer>
                    <pfx2:flight>
                        <pfx2:flightId>
                            <xsl:value-of select="$Start/Input/pfx:BookingEntry[$bookingIndex]/pfx:flightId"/>
                        </pfx2:flightId>
                        <pfx2:airline>
                            <xsl:value-of select="$Start/Input/pfx:BookingEntry[$bookingIndex]/pfx:airline"/>
                        </pfx2:airline>
                        <pfx2:from>
                            <xsl:value-of select="$Start/Input/pfx:BookingEntry[$bookingIndex]/pfx:from"/>
                        </pfx2:from>
                        <pfx2:to>
                            <xsl:value-of select="$Start/Input/pfx:BookingEntry[$bookingIndex]/pfx:to"/>
                        </pfx2:to>
                        <pfx2:date>
                            <xsl:value-of select="$Start/Input/pfx:BookingEntry[$bookingIndex]/pfx:date"/>
                        </pfx2:date>
                        <pfx2:scheduledDeparture>
                            <xsl:value-of select="$Start/Input/pfx:BookingEntry[$bookingIndex]/pfx:scheduledDeparture"/>
                        </pfx2:scheduledDeparture>
                        <pfx2:scheduledArrival>
                            <xsl:value-of select="$Start/Input/pfx:BookingEntry[$bookingIndex]/pfx:scheduledArrival"/>
                        </pfx2:scheduledArrival>
                    </pfx2:flight>
                </pfx2:FlightBookingRequestMessage>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="Generate Error">
            <pd:type>com.tibco.pe.core.GenerateErrorActivity</pd:type>
            <pd:resourceType>ae.activities.throw</pd:resourceType>
            <pd:x>717</pd:x>
            <pd:y>235</pd:y>
            <config>
                <faultName/>
            </config>
            <pd:inputBindings>
                <ns0:ActivityInput xmlns:ns0="http://www.tibco.com/pe/GenerateErrorActivity/InputSchema"/>
            </pd:inputBindings>
        </pd:activity>
    </pd:group>
    <pd:activity name="CatchAll">
        <pd:type>com.tibco.pe.core.CatchActivity</pd:type>
        <pd:resourceType>ae.activities.catch</pd:resourceType>
        <pd:x>786</pd:x>
        <pd:y>331</pd:y>
        <pd:handler>true</pd:handler>
        <config>
            <catchAll>true</catchAll>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:activity name="SendNegTravelBookingResponse">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>1071</pd:x>
        <pd:y>332</pd:y>
        <config>
            <processName>/IntegrationServices/TravelAgency/TravelAgencyResponseSender.process</processName>
        </config>
        <pd:inputBindings>
            <Input>
                <CorrelationId>
                    <xsl:value-of select="$Start/Input/CorrelationId"/>
                </CorrelationId>
                <MessageBody>
                    <xsl:value-of select="$RenderNegTravelBookingResponse/xmlString"/>
                </MessageBody>
            </Input>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="SendTravelBookingResponse">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>1073</pd:x>
        <pd:y>177</pd:y>
        <config>
            <processName>/IntegrationServices/TravelAgency/TravelAgencyResponseSender.process</processName>
        </config>
        <pd:inputBindings>
            <Input>
                <CorrelationId>
                    <xsl:value-of select="$Start/Input/CorrelationId"/>
                </CorrelationId>
                <MessageBody>
                    <xsl:value-of select="$RenderTravelBookingResponse/xmlString"/>
                </MessageBody>
            </Input>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="RenderTravelBookingResponse">
        <pd:type>com.tibco.plugin.xml.XMLRendererActivity</pd:type>
        <pd:resourceType>ae.activities.XMLRendererActivity</pd:resourceType>
        <pd:x>931</pd:x>
        <pd:y>175</pd:y>
        <config>
            <renderAsText>text</renderAsText>
            <validateInput>false</validateInput>
            <term ref="pfx:TravelBookingResponseMessage"/>
        </config>
        <pd:inputBindings>
            <pfx:TravelBookingResponseMessage>
                <pfx:correlationId>
                    <xsl:value-of select="$Start/Input/CorrelationId"/>
                </pfx:correlationId>
                <pfx:success>
                    <xsl:value-of select="true()"/>
                </pfx:success>
                <pfx:bookings>
                    <xsl:for-each select="$Start/Input/pfx:BookingEntry">
                        <pfx:booking>
                            <pfx:flightId>
                                <xsl:value-of select="pfx:flightId"/>
                            </pfx:flightId>
                            <pfx:airline>
                                <xsl:value-of select="pfx:airline"/>
                            </pfx:airline>
                            <pfx:from>
                                <xsl:value-of select="pfx:from"/>
                            </pfx:from>
                            <pfx:to>
                                <xsl:value-of select="pfx:to"/>
                            </pfx:to>
                            <pfx:date>
                                <xsl:value-of select="pfx:date"/>
                            </pfx:date>
                            <pfx:scheduledDeparture>
                                <xsl:value-of select="pfx:scheduledDeparture"/>
                            </pfx:scheduledDeparture>
                            <pfx:scheduledArrival>
                                <xsl:value-of select="pfx:scheduledArrival"/>
                            </pfx:scheduledArrival>
                        </pfx:booking>
                    </xsl:for-each>
                </pfx:bookings>
            </pfx:TravelBookingResponseMessage>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="RenderNegTravelBookingResponse">
        <pd:type>com.tibco.plugin.xml.XMLRendererActivity</pd:type>
        <pd:resourceType>ae.activities.XMLRendererActivity</pd:resourceType>
        <pd:x>936</pd:x>
        <pd:y>331</pd:y>
        <config>
            <renderAsText>text</renderAsText>
            <validateInput>false</validateInput>
            <term ref="pfx:TravelBookingResponseMessage"/>
        </config>
        <pd:inputBindings>
            <pfx:TravelBookingResponseMessage>
                <pfx:correlationId>
                    <xsl:value-of select="$Start/Input/CorrelationId"/>
                </pfx:correlationId>
                <pfx:success>
                    <xsl:value-of select="false()"/>
                </pfx:success>
                <pfx:bookings>
                    <xsl:for-each select="$Start/Input/pfx:BookingEntry">
                        <pfx:booking>
                            <pfx:flightId>
                                <xsl:value-of select="pfx:flightId"/>
                            </pfx:flightId>
                            <pfx:airline>
                                <xsl:value-of select="pfx:airline"/>
                            </pfx:airline>
                            <pfx:from>
                                <xsl:value-of select="pfx:from"/>
                            </pfx:from>
                            <pfx:to>
                                <xsl:value-of select="pfx:to"/>
                            </pfx:to>
                            <pfx:date>
                                <xsl:value-of select="pfx:date"/>
                            </pfx:date>
                            <pfx:scheduledDeparture>
                                <xsl:value-of select="pfx:scheduledDeparture"/>
                            </pfx:scheduledDeparture>
                            <pfx:scheduledArrival>
                                <xsl:value-of select="pfx:scheduledArrival"/>
                            </pfx:scheduledArrival>
                        </pfx:booking>
                    </xsl:for-each>
                </pfx:bookings>
            </pfx:TravelBookingResponseMessage>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>ForEachFlight</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>ForEachFlight</pd:from>
        <pd:to>RenderTravelBookingResponse</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>CatchAll</pd:from>
        <pd:to>RenderNegTravelBookingResponse</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>SendNegTravelBookingResponse</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>SendTravelBookingResponse</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>RenderTravelBookingResponse</pd:from>
        <pd:to>SendTravelBookingResponse</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>RenderNegTravelBookingResponse</pd:from>
        <pd:to>SendNegTravelBookingResponse</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>